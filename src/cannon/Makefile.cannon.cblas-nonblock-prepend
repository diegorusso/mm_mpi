include ../Makefile.mm.inc

OBJECTS = mm.o check.o prthead.o gendat.o prtspeed.o mxm.o \
          state.o

EXEC    = x.mm

${EXEC}: ${OBJECTS}
	@echo
	${LD_MPI} ${LDFLAGS} -o ${EXEC} ${OBJECTS} ${LIBS}

.SUFFIXES:  .c .o
.c.o    :
	@echo
	${CC_MPI} ${CFLAGS} -D__CBLAS -D__PREPOSTED_NONBLOCKING ${INCS} -c $<

clean   :
	/bin/rm -f ${OBJECTS} *.oo

veryclean:
	/bin/rm -f ${OBJECTS} *.oo ${EXEC}

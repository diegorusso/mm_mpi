include ../../Makefile.mm.inc

VPATH = ../shared

OBJECTS = mm.o check.o gendat.o mxm.o mxm-local.o \
          format.o reset.o utils.o

EXEC    = x.mm

${EXEC}: clean ${OBJECTS}
	@echo
	${LD_MPI} ${LDFLAGS} ${OBJECTS} ${LIBS} -o ${EXEC}

.SUFFIXES:  .c .o
.c.o    :
	@echo
	${CC_MPI} ${CFLAGS} -DOPENMP_INNER ${INCS} -c -o $@ $<

clean:
	/bin/rm -f ${OBJECTS} ${EXEC}
